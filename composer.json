{
    "name": "gpos/filament-jibble",
    "description": "Filament admin integration for the Jibble API.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0",
        "illuminate/console": "^11.0",
        "illuminate/contracts": "^11.0",
        "illuminate/cache": "^11.0",
        "illuminate/http": "^11.0",
        "illuminate/database": "^11.0",
        "filament/filament": "^3.2|^4.0",
        "livewire/livewire": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Gpos\\FilamentJibble\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gpos\\FilamentJibble\\Providers\\FilamentJibbleServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
